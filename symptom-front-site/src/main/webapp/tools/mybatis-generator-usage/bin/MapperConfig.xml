		<mapper resource="jp/co/toyata/vc/jimoties/repository/mybatis/TrnPostMapper.xml" />
		<mapper resource="jp/co/toyata/vc/jimoties/repository/mybatis/TrnSpamPostMapper.xml" />
		<mapper resource="jp/co/toyata/vc/jimoties/repository/mybatis/WrkAreaRankingMapper.xml" />
		<mapper resource="jp/co/toyata/vc/jimoties/repository/mybatis/WrkPushNewCommentMapper.xml" />
